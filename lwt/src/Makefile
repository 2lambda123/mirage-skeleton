PHONY: clean main.native

_build/.stamp:
	rm -rf _build
	mkdir -p _build/lib
	@touch $@

main.native: _build/.stamp
	ocamlbuild -classic-display -use-ocamlfind -lflag -linkpkg  -pkgs lwt.syntax,fd-send-recv,mirage -tags "syntax(camlp4o)" main.native

build: main.native
	@ :

clean:
	ocamlbuild -clean
